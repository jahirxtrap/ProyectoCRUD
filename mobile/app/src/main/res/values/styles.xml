        <item name="cornerSize">50%</item>
    </style>

    <style name="Widget.App.Snackbar" parent="Widget.Material3.Snackbar">
        <item name="android:backgroundTint">?attr/colorSurfaceContainerHigh</item>
        <item name="android:layout_margin">16dp</item>
        <item name="elevation">@dimen/m3_sys_elevation_level2</item>
        <item name="shapeAppearance">?attr/shapeAppearanceCornerLarge</item>
